{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "dest": "/pt/index.html"
    },
    {
      "src": "/contact",
      "dest": "/api/contact",
      "methods": ["POST"]
    }
  ],
  "env": {
    "LEADCAPTURE_DISCORD_ENDPOINT": "@leadcapture_discord_endpoint",
    "RECAPTCHA_SECRET": "@recaptcha_secret"
  },
  "build": {
    "env": {
      "RECAPTCHA_PUBLIC": "@recaptcha_public"
    }
  }
}